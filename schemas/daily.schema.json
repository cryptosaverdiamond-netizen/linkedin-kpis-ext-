{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["type", "company_id", "team_id", "user_id"],
  "properties": {
    "type": {
      "type": "string",
      "enum": ["daily"]
    },
    "company_id": {
      "type": "string",
      "minLength": 1
    },
    "team_id": {
      "type": "string", 
      "minLength": 1
    },
    "user_id": {
      "type": "string",
      "minLength": 1
    },
    "date_yyyy_mm_dd": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "global_posts_impressions_last_7d": {
      "type": "number",
      "minimum": 0
    },
    "followers": {
      "type": "number",
      "minimum": 0
    },
    "profile_views_90d": {
      "type": "number",
      "minimum": 0
    },
    "search_appearances_last_week": {
      "type": "number", 
      "minimum": 0
    },
    "source_file": {
      "type": "string"
    },
    "captured_at_iso": {
      "type": "string",
      "format": "date-time"
    },
    "trace_id": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-4[0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$"
    }
  },
  "additionalProperties": false
}