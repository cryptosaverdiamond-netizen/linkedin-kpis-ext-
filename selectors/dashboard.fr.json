{
  "kpi_selectors": {
    "global_posts_impressions_last_7d": [
      "p.text-body-large-bold.t-black",
      ".text-body-large-bold",
      "p[class*='text-body-large-bold']",
      "div[class*='analytics'] p.text-body-large-bold",
      "span.text-body-xsmall + p.text-body-large-bold",
      "[class*='analytics-tools'] p.text-body-large-bold"
    ],
    "followers": [
      "p.text-body-large-bold.t-black",
      ".text-body-large-bold",
      "p[class*='text-body-large-bold']",
      "div[class*='followers'] p.text-body-large-bold",
      "div[class*='connections'] p.text-body-large-bold",
      "span.text-body-xsmall + p.text-body-large-bold"
    ],
    "profile_views_90d": [
      "p.text-body-large-bold.t-black",
      ".text-body-large-bold",
      "p[class*='text-body-large-bold']",
      "div[class*='profile-views'] p.text-body-large-bold",
      "div[class*='views'] p.text-body-large-bold",
      "span.text-body-xsmall + p.text-body-large-bold"
    ],
    "search_appearances_last_week": [
      "p.text-body-large-bold.t-black",
      ".text-body-large-bold", 
      "p[class*='text-body-large-bold']",
      "div[class*='search'] p.text-body-large-bold",
      "div[class*='appearances'] p.text-body-large-bold",
      "span.text-body-xsmall + p.text-body-large-bold"
    ]
  },
  "fallback_selectors": {
    "impressions_pattern": [
      "\\d+\\s*impressions?",
      "\\d+\\s*vues?",
      "\\d+\\s*views?"
    ],
    "followers_pattern": [
      "\\d+\\s*followers?",
      "\\d+\\s*abonnés?",
      "\\d+\\s*connections?"
    ],
    "profile_views_pattern": [
      "\\d+\\s*vues?\\s*de\\s*profil",
      "\\d+\\s*profile\\s*views?",
      "\\d+\\s*vues?"
    ],
    "search_pattern": [
      "\\d+\\s*apparitions?\\s*dans\\s*les\\s*recherches",
      "\\d+\\s*search\\s*appearances?",
      "\\d+\\s*recherches?"
    ]
  },
  "text_indicators": {
    "french_keywords": [
      "impressions",
      "followers",
      "vues de profil",
      "apparitions dans les recherches",
      "abonnés",
      "recherches"
    ],
    "english_keywords": [
      "impressions",
      "followers", 
      "profile views",
      "search appearances",
      "connections",
      "searches"
    ]
  }
}