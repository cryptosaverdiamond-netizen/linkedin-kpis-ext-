{
  "post_selectors": {
    "post_container": [
      ".feed-shared-update-v2"
    ],
    "post_id": [
      ".feed-shared-update-v2[data-urn]",
      "[data-urn*='urn:li:activity:']",
      "div[data-urn]",
      "article[data-urn]",
      "[href*='urn:li:activity:']",
      "[id*='urn:li:activity:']"
    ],
    "created_at": [
      "[class*=\"date\"]",
      ".update-components-actor__sub-description",
      "time[datetime]",
      ".update-components-actor__sub-description time",
      "span.update-components-actor__sub-description",
      ".feed-shared-actor__sub-description",
      "time"
    ],
    "impressions": [
      ".social-details-social-counts__reactions-count",
      "span[class*='social-counts']",
      "button[data-reaction-details] span",
      "[class*='impressions']",
      "[aria-label*='impressions']"
    ],
    "reactions": [
      "button[aria-label*='réactions']",
      ".social-details-social-counts__reactions-count",
      ".social-details-social-counts__count-value",
      "[aria-label*='réactions']",
      "[class*='reactions-count']"
    ],
    "comments": [
      "button[aria-label*='commentaires']",
      ".social-details-social-counts__comments",
      "[aria-label*='commentaires']",
      "[class*='comments-count']",
      "button[class*='comment'] span"
    ],
    "shares": [
      "button[aria-label*='republications']",
      "button[aria-label*='partages']",
      ".social-details-social-counts__shares",
      "[aria-label*='republications']",
      "[aria-label*='partages']",
      "[class*='shares-count']"
    ],
    "reshares": [
      "button[aria-label*='republications']",
      "button[aria-label*='repartages']",
      ".social-details-social-counts__reshares",
      "[aria-label*='republications']",
      "[aria-label*='repartages']",
      "[class*='reshares-count']"
    ]
  },
  "repost_indicators": {
    "text_patterns": [
      "repost",
      "repartage",
      "partagé",
      "reposté",
      "reposté par",
      "shared by",
      "reposted by"
    ],
    "visual_indicators": [
      ".repost-indicator",
      ".shared-indicator",
      "[data-test-id='repost-indicator']",
      ".feed-shared-update-v2--repost"
    ]
  },
  "language_detection": {
    "french_indicators": [
      "bonjour",
      "salut",
      "merci",
      "très",
      "avec",
      "pour",
      "dans",
      "sur",
      "par",
      "de",
      "la",
      "le",
      "les",
      "un",
      "une",
      "et",
      "ou",
      "mais",
      "donc",
      "car"
    ],
    "english_indicators": [
      "hello",
      "hi",
      "thanks",
      "very",
      "with",
      "for",
      "in",
      "on",
      "by",
      "the",
      "a",
      "an",
      "and",
      "or",
      "but",
      "so",
      "because"
    ]
  },
  "fallback_patterns": {
    "impressions": [
      "\\d+\\s*impressions?",
      "\\d+\\s*vues?",
      "\\d+\\s*views?"
    ],
    "reactions": [
      "\\d+\\s*réactions?",
      "\\d+\\s*reactions?",
      "\\d+\\s*likes?"
    ],
    "comments": [
      "\\d+\\s*commentaires?",
      "\\d+\\s*comments?",
      "\\d+\\s*réponses?"
    ],
    "shares": [
      "\\d+\\s*partages?",
      "\\d+\\s*shares?",
      "\\d+\\s*repartages?"
    ]
  }
}